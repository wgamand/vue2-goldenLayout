{
  "name": "vue2-golden-layout",
  "version": "0.1.0",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve",
    "dev": "vue-cli-service serve ./examples/main.js",
    "build": "vue-cli-service build",
    "build-library": "vue-cli-service build --target lib --name vue-golden-layout --entry src/index.js",
    "test:unit": "vue-cli-service test:unit",
    "test:coverage": "jest --coverage",
    "lint": "vue-cli-service lint"
  },
  "main": "dist/vue-golden-layout.common.js",
  "dependencies": {
    "element-ui": "^2.15.6",
    "golden-layout": "1.5.9",
    "jquery": "^3.6.0",
    "vue": "^2.6.11",
    "vue-router": "^3.5.3",
    "vue-splitpane": "^1.0.6"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/server-test-utils": "^1.3.0",
    "@vue/test-utils": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "compression-webpack-plugin": "^1.1.12",
    "electron": "6.0.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "vue-cli-plugin-electron-builder": "^1.4.0",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "files": [
    "docs/*",
    "src/*",
    "assets/*"
  ],
  "keywords": [
    "vue",
    "golden-layout"
  ],
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "license": "MIT",
  "description": "A multi-screen javascript Layout manager",
  "directories": {
    "example": "examples",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wgamand/vue2-goldenLayout.git"
  },
  "homepage": "https://github.com/wgamand/vue2-goldenLayout",
  "author": "zhaoaijing",
  "bugs": {
    "url": "https://github.com/wgamand/vue2-goldenLayout/issues"
  }
}
